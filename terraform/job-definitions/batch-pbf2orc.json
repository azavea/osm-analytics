{
    "image": "${batch_image_url}",
    "vcpus": 4,
    "memory": 8192,
    "command": [
        "/tmp/data/${pbf_file}",
        "/tmp/data/${output_tag}.osh.orc"
    ],
    "volumes": [
        {
            "host": {
                "sourcePath": "/media/ephemeral0"
            },
            "name": "ephemeral0"
        }
    ],
    "environment": [
        {
            "name": "AWS_DEFAULT_REGION",
            "value": "${aws_region}"
        },
        {
            "name": "STATUS_PATH",
            "value": "${status_path}"
        },
        {
            "name": "PBF_PATH",
            "value": "${pbf_path}"
        },
        {
            "name": "ORC_PATH",
            "value": "${orc_path}"
        },
        {
            "name": "ENVIRONMENT",
            "value": "${environment}"
        }
    ],
    "mountPoints": [
        {
            "containerPath": "/tmp/data",
            "readOnly": false,
            "sourceVolume": "ephemeral0"
        }
    ]
}
